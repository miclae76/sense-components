{
  "name": "widget-components",
  "description": "Components to be used in Widgets in Qlik Sense.",
  "version": "0.1.1",
  "homepage": "https://github.com/stefanwalther/widget-components",
  "author": "Stefan Walther (http://qliksite.io)",
  "repository": "stefanwalther/widget-components",
  "bugs": {
    "url": "https://github.com/stefanwalther/widget-components/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "keywords": [
    "components",
    "widget"
  ],
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 2
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ]
  },
  "devDependencies": {
    "angular": "^1.5.8",
    "angularjs": "0.0.1",
    "karma": "^1.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-phantomjs2-launcher": "^0.5.0",
    "karma-requirejs": "^1.0.0",
    "mocha": "^3.0.0",
    "requirejs": "^2.2.0"
  }
}
